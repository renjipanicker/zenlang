namespace Url;

public struct url {
    string fullUrl;
    string protocol;
    string host;
    string port;
    string path;
    string querystring;
};

public routine bool Exists(const url& u) native;
public routine bool FileExists(const string& urlstr) native;
public routine void Parse(url& u, const string& urlstr) native;
public routine url Create(const string& urlstr) native;
